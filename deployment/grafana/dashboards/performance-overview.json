{
  "dashboard": {
    "id": null,
    "title": "Payment Platform Performance Overview",
    "tags": ["performance", "payment-platform"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 39,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "API Response Times",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "off"}
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 1000}
              ]
            },
            "unit": "ms"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {"mode": "single", "sort": "none"}
        },
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "rate(api_response_duration_seconds_sum[5m]) / rate(api_response_duration_seconds_count[5m]) * 1000",
            "interval": "",
            "legendFormat": "Average Response Time",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "histogram_quantile(0.95, rate(api_response_duration_seconds_bucket[5m])) * 1000",
            "interval": "",
            "legendFormat": "95th Percentile",
            "refId": "B"
          },
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "histogram_quantile(0.99, rate(api_response_duration_seconds_bucket[5m])) * 1000",
            "interval": "",
            "legendFormat": "99th Percentile",
            "refId": "C"
          }
        ]
      },
      {
        "id": 2,
        "title": "Database Query Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "rate(database_query_duration_seconds_sum[5m]) / rate(database_query_duration_seconds_count[5m]) * 1000",
            "legendFormat": "Average Query Time",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "database_queries_slow_total",
            "legendFormat": "Slow Queries",
            "refId": "B"
          }
        ]
      },
      {
        "id": 3,
        "title": "Cache Performance",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "cache_hits_total",
            "legendFormat": "Cache Hits",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "cache_misses_total",
            "legendFormat": "Cache Misses",
            "refId": "B"
          }
        ]
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "max": 2048,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1024},
                {"color": "red", "value": 1536}
              ]
            },
            "unit": "decbytes"
          }
        },
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "jvm_memory_used_bytes{area=\"heap\"} / 1024 / 1024",
            "legendFormat": "Heap Memory MB",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Active Sessions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "spring_session_active_sessions",
            "legendFormat": "Active Sessions",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Request Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "rate(http_server_requests_seconds_count[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Error Rate by Endpoint",
        "type": "table",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "rate(http_server_requests_seconds_count{status=~\"[45].*\"}[5m])",
            "legendFormat": "{{method}} {{uri}}",
            "refId": "A",
            "format": "table"
          }
        ]
      },
      {
        "id": 8,
        "title": "Performance Alerts Timeline",
        "type": "logs",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "ALERTS{alertname=~\".*Performance.*\"}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 18},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "hikaricp_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "hikaricp_connections_idle",
            "legendFormat": "Idle Connections",
            "refId": "B"
          },
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "hikaricp_connections_max",
            "legendFormat": "Max Connections",
            "refId": "C"
          }
        ]
      },
      {
        "id": 10,
        "title": "Security Events",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 18},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "increase(security_events_total[1h])",
            "legendFormat": "Security Events/hour",
            "refId": "A"
          }
        ]
      },
      {
        "id": 11,
        "title": "Payment Processing Rate",
        "type": "graph",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 18},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
            "expr": "rate(payment_transactions_total[5m])",
            "legendFormat": "Payments/sec",
            "refId": "A"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "Prometheus",
            "value": "Prometheus"
          },
          "hide": 0,
          "includeAll": false,
          "label": "Data Source",
          "multi": false,
          "name": "DS_PROMETHEUS",
          "options": [],
          "query": "prometheus",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "type": "datasource"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {"type": "datasource", "uid": "grafana"},
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
          "enable": true,
          "expr": "ALERTS{alertname=~\".*Performance.*\"}",
          "iconColor": "red",
          "name": "Performance Alerts",
          "step": "60s",
          "tagKeys": "alertname",
          "textFormat": "{{alertname}}: {{summary}}",
          "titleFormat": "Performance Alert"
        }
      ]
    }
  }
}