{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "validate": "npm run lint:md && npm run lint:links",
    "lint": "npm run lint:md && npm run lint:code",
    "lint:md": "markdownlint-cli docs/**/*.md",
    "lint:links": "markdown-link-check docs/**/*.md --config .markdown-link-check.json",
    "lint:code": "eslint src --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"**/*.{md,js,ts,json}\"",
    "docs-build": "npm run validate && npm run build",
    "docs-serve": "npm run serve",
    "docs-validate": "npm run validate",
    "docs-create": "node scripts/create-doc.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@docsearch/react": "^4.0.1",
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "markdown-link-check": "^3.13.7",
    "markdownlint-cli": "^0.45.0",
    "supertest": "^7.1.4",
    "textlint": "^15.2.2",
    "ts-jest": "^29.4.2",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
